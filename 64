#include "stdafx.h"
int ack(int m,int n);
int main(int argc, char* argv[])
{
	int m,n,result;

	scanf("%d%d",&m,&n);

	result=ack(m,n);

	printf("%d\n",result);

	return 0;
}

int ack(int m,int n)
{
	if(m==0)
		return n+1;
	if((n==0) && (m>0))
		return ack(m-1,1);
	if((m>0) && (n>0))
		return ack(m-1,ack(m,n-1));
}
