1.欧几里得算法求最大公约数
#include "stdafx.h"

int main(int argc, char* argv[])
{
	
	int a,b,c,t,z;

	scanf("%d%d",&a,&b);
	z=b;
	if(b>a)
	{
		c=a,a=b,b=c;
		z=b;
	}

	for(c=a%b;c!=0;)
	{
		if(c>b)
		{
			c=c%b;
		}
		else
		{
			t=c;
			c=b%c;
			b=t;
		}
	};
		
	printf("%d和%d最大公约数%d\n",a,z,b);
	return 0;
}

2.1 普通算法求n以内的素数

#include "stdafx.h"

int main(int argc, char* argv[])
{

	int a[10000],n,i,k,s;

	scanf("%d",&n);

	for(i=1;i<=n;i++)
		a[i]=i;
		
	a[1]=0;
	
	for(i=2;i<=n;i++)
	{
		s=1;
		
		for(k=2;s!=0;k++)
		s=a[i]%k;
		
		if(k<a[i])
		a[i]=0;
	}

	for(i=1;i<=n;i++)
		if(a[i]!=0)
			printf("%d ",a[i]);
	return 0;
}
