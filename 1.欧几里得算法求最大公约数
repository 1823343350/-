1.欧几里得算法求最大公约数
#include "stdafx.h"

int main(int argc, char* argv[])
{
	
	int a,b,c,t,z;

	scanf("%d%d",&a,&b);
	z=b;
	if(b>a)
	{
		c=a,a=b,b=c;
		z=b;
	}

	for(c=a%b;c!=0;)
	{
		if(c>b)
		{
			c=c%b;
		}
		else
		{
			t=c;
			c=b%c;
			b=t;
		}
	};
		
	printf("%d和%d最大公约数%d\n",a,z,b);
	return 0;
}

